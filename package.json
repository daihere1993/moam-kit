{
  "name": "moam-kit",
  "version": "0.0.1",
  "main": "./dist/main-process/main.js",
  "author": {
    "name": "Luke Wu",
    "email": "daihere1993@gmail.com"
  },
  "description": "moam-kit from luke",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "start:win": "npm-run-all -p ng:serve electron:serve-win",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:prod": "npm run build -- -c production",
    "test": "npm-run-all -s test:main test:render",
    "test:main": "jest --config ./jest.config-main.json",
    "test:render": "jest --config ./jest.config-render.json",
    "test:main-dev": "jest --config ./jest.config-main.json --watch",
    "test:render-dev": "jest --config ./jest.config-render.json --watch",
    "test:main-coverage": "jest --config ./jest.config-main.json --coverage",
    "test:render-coverage": "jest --config ./jest.config-render.json --coverage",
    "ng:serve": "ng serve --port 1337",
    "electron:serve": "wait-on http-get://localhost:1337/ && npm run electron:serve-tsc && electron . --serve",
    "electron:serve-win": "wait-on http-get://localhost:1337/ && npm run electron:serve-tsc && node_modules/.bin/electron.cmd . --serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --write \"**/*.ts\" \"**/*.json\" \"**/*.js\" \"**/*.html\" \"**/*.scss\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "rxjs": "~6.5.4",
    "shelljs": "^0.8.4",
    "ssh2-sftp-client": "^5.1.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.11.1",
    "@types/shelljs": "^0.8.7",
    "@types/ssh2-sftp-client": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "codelyzer": "^5.1.2",
    "devtron": "^1.4.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-reload": "^1.5.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^19.0.1",
    "jest": "^26.0.1",
    "jest-preset-angular": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "typescript": "~3.7.5",
    "wait-on": "^5.0.0"
  }
}
