{
  "name": "moam-kit",
  "version": "0.0.1",
  "main": "./dist/main-process/main.js",
  "author": {
    "name": "Luke Wu",
    "email": "daihere1993@gmail.com"
  },
  "description": "moam-kit from luke",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "run-p \"ng:serve --port 1337\" electron:serve",
    "dev": "run-p \"ng:serve --port 1337\" electron:serve-tsc-dev",
    "ng:serve": "ng serve",
    "testcafe": "wait-on http-get://localhost:3200/ && testcafe",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:prod": "npm run build -- -c production",
    "e2e": "run-p \"ng:serve --port 3200\" testcafe",
    "e2e-dev": "run-p \"ng:serve --port 3200\" \"testcafe --live\"",
    "test:app": "jest --config ./jest.config-render.json",
    "test:app-dev": "run-s \"test:app --watch\"",
    "test:main": "jest --config ./jest.config-main.json",
    "test:main-dev": "run-s \"test:main --watch\"",
    "electron:serve": "wait-on http-get://localhost:1337/ && npm run electron:serve-tsc && electron . --serve",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve-tsc-dev": "tsc -p tsconfig.serve.json -w",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --write \"**/*.ts\" \"**/*.json\" \"**/*.js\" \"**/*.html\" \"**/*.scss\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "ng-zorro-antd": "^9.1.2",
    "rxjs": "~6.5.4",
    "seedrandom": "^3.0.5",
    "shelljs": "^0.8.4",
    "ssh2-sftp-client": "^5.1.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@schematics/angular": "~9.0.1",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.18",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.156",
    "@types/node": "^12.11.1",
    "@types/request": "^2.48.5",
    "@types/seedrandom": "^2.4.28",
    "@types/shelljs": "^0.8.7",
    "@types/ssh2-sftp-client": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "codelyzer": "^5.1.2",
    "devtron": "^1.4.0",
    "electron": "^18.3.7",
    "electron-builder": "^22.6.0",
    "electron-reload": "^1.5.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^19.0.1",
    "jest": "^26.0.1",
    "jest-preset-angular": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "testcafe": "^1.8.6",
    "ts-jest": "^26.1.0",
    "ts-node": "~8.3.0",
    "typescript": "~3.7.5",
    "wait-on": "^5.0.0"
  }
}
